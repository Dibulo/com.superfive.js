{
  "name": "com.superfive.js",
  "version": "1.0.3",
  "type": "module",
  "main": "./dist/main.js",
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "types": "./dist/main.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "description": "The JS API for the SuperFive.io gaming platform where players can compete in a hypercasual games tournament",
  "author": "Dibulo",
  "license": "MIT",
  "homepage": "https://superfive.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dibulo/com.superfive.js.git"
  },
  "bugs": {
    "url": "https://github.com/Dibulo/com.superfive.js/issues"
  },
  "keywords": [
    "superfive",
    "gaming",
    "js",
    "api"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -p tsconfig.build.json && vite build",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "preview": "vite preview",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.1.14"
  },
  "pnpm": {
    "overrides": {
      "vite": "npm:rolldown-vite@7.1.14"
    }
  }
}
